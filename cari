#include <stdio.h>

main()
{
    int  i, j, x=3, y, a[2][5]={4, 5, 8, 1, 2,
                                3, 11, 23, 10, 13};

    printf("\t\t    Mencari index bilangan 3 pada matrix A[2][5]\n\t\t\t\t4   5   8   1   2 \n\t\t\t\t3  11  23  10  13\n");

    for(i=0; i<2; i++)
    {
        for(j=0; j<5; j++)
        {
            y = a[i][j];

            if(a[i][j]==x)
                {
                    printf("\n\nBilangan 3 ditemukan pada index [%d][%d]\n", i, j, a[i][j]);
                }
            /*else
                {
                    printf("\nBilangan %d ditemukan pada index ini", y);
                    printf("[%d][%d]", i, j, a[i][j]);
                }*/
        }
    }
}
